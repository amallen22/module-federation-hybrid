{
  "method": "GET",
  "path": "/document",
  "params": {
    "documentType": {
      "description": "documentType",
      "type": "string",
      "allowEmpty": false,
      "required": true
    },
    "limit": {
      "description": "limits the docs returned by the query",
      "type": "number",
      "allowEmpty": false,
      "required": false
    },
    "offset": {
      "description": "offset for pagination",
      "type": "number",
      "allowEmpty": true,
      "required": false
    }
  },
  "logOutOnCodeRanges": [
    {
      "min": 400,
      "max": 499
    }
  ]
}