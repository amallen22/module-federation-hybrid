{
    "name": "@cv-hibrid/login",
    "version": "2.1.0",
    "description": "cv-app-login with React 18, Vite and Module Federation",
    "type": "module",
    "config": {
        "cvTranslationsRepository": "git@bitbucket.org:grupoblidoo/cv-translations.git",
        "cvTranslationsBranch": "master",
        "cvTranslationsFolder": "cv-translations",
        "middleEndScript": "updateMiddleEndTranslations.sh",
        "i18nFolder": "src/app/i18n"
    },
    "scripts": {
        "setup": "node $NODE_DEBUG_OPTION ./node_modules/@npm_leadtech/cv-lib-app-config/src/scripts/run.js",
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview --port 5003",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "test:coverage": "vitest --coverage",
        "updateTranslations": "npm run getMemsource && npm run unzipMemsource && npm run mergeMemsource",
        "getMemsource": "git archive --remote=$npm_package_config_cvTranslationsRepository --format=zip --output=$npm_package_config_cvTranslationsFolder\".zip\" $npm_package_config_cvTranslationsBranch",
        "unzipMemsource": "unzip $npm_package_config_cvTranslationsFolder\".zip\" -d $npm_package_config_cvTranslationsFolder && rm $npm_package_config_cvTranslationsFolder\".zip\"",
        "mergeMemsource": "./$npm_package_config_cvTranslationsFolder/$npm_package_config_middleEndScript -a $npm_package_name -i $npm_package_config_i18nFolder && rm -r $npm_package_config_cvTranslationsFolder",
        "eslint": "eslint ./src --ext js,jsx,ts,tsx",
        "eslint:fix": "eslint ./src --ext js,jsx,ts,tsx --fix"
    },
    "author": "LeadTech",
    "dependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^5.18.0",
        "@npm_leadtech/cv-lib-app-analytics": "1.1.8",
        "@npm_leadtech/cv-lib-app-components": "5.0.8",
        "@npm_leadtech/cv-lib-app-config": "3.26.55",
        "@npm_leadtech/cv-lib-app-jsnlog": "2.0.8",
        "@npm_leadtech/cv-lib-auth": "1.0.3",
        "@npm_leadtech/cv-lib-cookie-channel-context": "1.1.2",
        "@npm_leadtech/cv-lib-social": "2.7.6",
        "@npm_leadtech/cv-lib-visitor": "2.3.7-2",
        "@npm_leadtech/cv-storage-js": "3.3.3-amplitude.2",
        "@npm_leadtech/cv-tests-app-common": "2.2.2",
        "@npm_leadtech/jsr-lib-http": "2.1.1-visitor.0",
        "@packages/auth": "workspace:*",
        "@packages/query": "workspace:*",
        "@tanstack/react-query": "^5.62.13",
        "amazon-cognito-identity-js": "~2.0",
        "buffer": "^6.0.3",
        "zustand": "^5.0.9",
        "classnames": "2.2.6",
        "core-js": "3.6.5",
        "counterpart": "0.18",
        "html-react-parser": "0.10.5",
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-is": "^18.3.1",
        "revalidator": "0.3.1",
        "sass": "^1.77.4",
        "sha1": "1.1.1",
        "url-polyfill": "1.1.5",
        "whatwg-fetch": "3.0.0"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/eslint-parser": "^7.26.0",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-classes": "^7.23.0",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-react": "^7.26.0",
        "@emotion/babel-plugin": "^11.11.0",
        "@originjs/vite-plugin-federation": "^1.4.1",
        "@testing-library/jest-dom": "^6.1.5",
        "@testing-library/react": "^14.1.2",
        "@types/react": "^18.3.23",
        "@types/react-dom": "^18.3.7",
        "@typescript-eslint/eslint-plugin": "^8.0.0",
        "@typescript-eslint/parser": "^8.0.0",
        "@vitejs/plugin-react": "^4.4.1",
        "eslint": "^8.57.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-react": "^7.34.1",
        "eslint-plugin-simple-import-sort": "^12.1.0",
        "jsdom": "^23.0.1",
        "typescript": "~5.8.3",
        "vite": "^6.3.5",
        "vite-plugin-node-polyfills": "^0.24.0",
        "vitest": "^1.6.0"
    }
}
