{
  "name": "@cv-hibrid/login",
  "version": "2.1.0",
  "description": "login with React, Vite and Module Federation",
  "type": "module",
  "config": {
    "cvTranslationsRepository": "git@bitbucket.org:grupoblidoo/cv-translations.git",
    "cvTranslationsBranch": "master",
    "cvTranslationsFolder": "cv-translations",
    "middleEndScript": "updateMiddleEndTranslations.sh",
    "i18nFolder": "src/app/i18n"
  },
  "scripts": {
    "setup": "node $NODE_DEBUG_OPTION ./node_modules/@npm_leadtech/cv-lib-app-config/src/scripts/run.js",
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview --port 5003",
    "test": "jest --config=jest.config.cjs",
    "test-watch": "jest --watch",
    "test-coverage": "jest --coverage",
    "test-coverage-watch": "jest --watch --coverage",
    "eslint": "eslint ./src --ext js,jsx,ts,tsx",
    "eslint:fix": "eslint ./src --ext js,jsx,ts,tsx --fix",
    "reinstall": "rm -f pnpm-lock.yaml && rm -Rf node_modules && pnpm i",
    "updateTranslations": "pnpm run getMemsource && pnpm run unzipMemsource && pnpm run mergeMemsource",
    "getMemsource": "git archive --remote=$npm_package_config_cvTranslationsRepository --format=zip --output=$npm_package_config_cvTranslationsFolder\".zip\" $npm_package_config_cvTranslationsBranch",
    "unzipMemsource": "unzip $npm_package_config_cvTranslationsFolder\".zip\" -d $npm_package_config_cvTranslationsFolder && rm $npm_package_config_cvTranslationsFolder\".zip\"",
    "mergeMemsource": "./$npm_package_config_cvTranslationsFolder/$npm_package_config_middleEndScript -a $npm_package_name -i $npm_package_config_i18nFolder && rm -r $npm_package_config_cvTranslationsFolder"
  },
  "author": "LeadTech",
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.4",
    "@mui/styles": "^5.13.2",
    "@mui/system": "^5.15.15",
    "@npm_leadtech/cv-lib-app-analytics": "1.1.8",
    "@npm_leadtech/cv-lib-app-components": "5.0.8",
    "@npm_leadtech/cv-lib-app-config": "3.26.55",
    "@npm_leadtech/cv-lib-app-jsnlog": "2.0.8",
    "@npm_leadtech/cv-lib-auth": "1.0.3",
    "@npm_leadtech/cv-lib-cookie-channel-context": "1.1.2",
    "@npm_leadtech/cv-lib-social": "2.6.6",
    "@npm_leadtech/cv-lib-visitor": "2.3.6",
    "@npm_leadtech/cv-storage-js": "3.3.3-amplitude.0",
    "@npm_leadtech/cv-tests-app-common": "2.2.2",
    "@npm_leadtech/jsr-lib-http": "2.1.1-visitor.0",
    "amazon-cognito-identity-js": "^4.6.3",
    "buffer": "^6.0.3",
    "classnames": "2.2.6",
    "core-js": "3.6.5",
    "counterpart": "0.18",
    "crypto-browserify": "^3.12.1",
    "crypto-js": "^4.2.0",
    "html-react-parser": "0.10.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "react-test-renderer": "^18.3.1",
    "revalidator": "0.3.1",
    "sass": "^1.77.4",
    "sha1": "1.1.1",
    "stream-browserify": "^3.0.0",
    "url-polyfill": "1.1.5",
    "whatwg-fetch": "3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-syntax-jsx": "^7.27.1",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-classes": "^7.28.0",
    "@babel/plugin-transform-object-rest-spread": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@emotion/babel-plugin": "^11.13.5",
    "@originjs/vite-plugin-federation": "^1.4.1",
    "@types/node": "^22.15.29",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.4.1",
    "babel-jest": "^30.0.4",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-babel": "^1.3.2",
    "vite-plugin-jsx": "^0.0.6"
  }
}
